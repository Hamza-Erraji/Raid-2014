		$('#pic111').animate({
		opacity: 1.0,
		top: '+=10',
		width:'30%',	
		height:'60%',
		}, 1200, function() {
		// Animation complete.
		});

		c='111';
		
		$('#next').click(function() {
		chaine='#pic';	
		prec=chaine.concat(c);

		if(c=='11111') c='';
		
		//c = c.slice(0, -1)
		c=c+'1',
		suiv=chaine.concat(c),
		$(suiv).animate({
		opacity: 1.0,
		top: '+=10',
		width:'30%',	
		height:'60%',
		
		}, 1000, function() {
		// Animation complete.
		});
		$(prec).animate({
		opacity: 0.4,
		top: '-=10',
		width:'6%',	
		height:'14%',
		
		}, 1000, function() {
		// Animation complete.
		});
		});




		$('#prev').click(function() {
		chaine='#pic';		
		suiv=chaine.concat(c);
		if(c=='1') c='111111';
		c = c.slice(0, -1)
		
		prec=chaine.concat(c),
		$(prec).animate({
		opacity: 1.0,
		top: '+=10',
		width:'30%',	
		height:'60%',
		
		}, 1000, function() {
		// Animation complete.
		});
